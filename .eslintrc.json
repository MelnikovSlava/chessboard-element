{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "no-prototype-builtins": "off",
    "no-console": "off",
    "no-var": "error",
    "prefer-const": "error",
    "semi": "error",
    "strict": "error",
    "no-useless-escape": "off"
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
